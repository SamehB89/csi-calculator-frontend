{
  "buildCommand": "echo 'No build needed for static site'",
  "outputDirectory": ".",
  "redirects": [
    {
      "source": "/:path*",
      "destination": "https://csi-calc.vercel.app/:path*",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "csi-calculator-frontend.vercel.app"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://web-production-90db2.up.railway.app/api/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        }
      ]
    }
  ]
}
